{
  "name": "idm.in",
  "version": "0.0.0",
  "description": "An open-source URL shortener to create shorten link with Cloudflare worker and pages. Deploy on Cloudflare for custom domain for free!",
  "scripts": {
    "ng": "ng",
    "devstart": "wrangler pages dev ./public --compatibility-date=2023-10-22 --kv=SHORTLINKS",
    "build": "ng build --configuration production --aot",
    "postbuild": "ncp ./public dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "dayjs": "^1.11.10",
    "hono": "^4.6.3",
    "nanoid": "^5.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "wrangler": "^3.78.12",
    "zod": "^3.22.4",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@cloudflare/workers-types": "^4.20240925.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ncp": "^2.0.0",
    "typescript": "~5.1.3"
  }
}
